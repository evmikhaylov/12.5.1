<Window x:Class="_12._5._3.ChangeInfoClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_12._5._3"
        mc:Ignorable="d"
        Title="ChangeInfoClientWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="Введите фамилию"/>
        <TextBox x:Name="NewFamilyName"/>
        <TextBlock Text="Введите имя"/>
        <TextBox x:Name="NewFirstName"/>
        <TextBlock Text="Введите отчество"/>
        <TextBox x:Name="NewPatronymic"/>
        <TextBlock Text="Введите номер телефона"/>
        <TextBox x:Name="NewNumberPhone"/>
        <TextBlock Text="Введите серию документа"/>
        <TextBox x:Name="NewSerialDoc"/>
        <TextBlock Text="Введите номер документа"/>
        <TextBox x:Name="NewNumberDoc"/>
        <Button x:Name="Ok" Content="Ok" Click="Ok_Click"/>
    </StackPanel>
</Window>
